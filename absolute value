#include <iostream>
using namespace std;
int main() {
    double num;
    cout << "Enter a number.\n";
    cin >> num;
    if (num >= 0){
        cout << "Your absolute value is " << num << "\n";
    } else if ( num < 0 ){
        cout << "Your absolute value is " << (-1)*num;
    }
    return 0;
}
