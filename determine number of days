#include <iostream>
using namespace std;
int main() {
    int days, num;
    cout << "Enter the number of days from a sunday to determine the day";
    cin >> days;
    num = days%7;
    
    switch (num){
        case 0:
            cout << "Sunday";
            break;
        case 1:
            cout << "Monday";
            break;
        case 2:
            cout << "Tuesday";
            break;
        case 3:
            cout << "Wednesday";
            break;
        case 4:
            cout << "Thursday";
            break;
        case 5:
            cout << "Friday";
            break;
        case 6:
            cout << "Saturday";
            break;
        
    }
    

    return 0;
}
