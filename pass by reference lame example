#include <iostream>
using namespace std;
void zero_small(int &a, int &b);
int main(){
    int num1, num2;
    cout<<"Enter the first number"<<endl;
    cin>>num1;
    cout<<"Enter the second number"<<endl;
    cin>>num2;
    zero_small(num1, num2);
    return 0;  
}
void zero_small(int &a, int &b){
    if (a<b){
        a=0;
    } else b=0;
    cout<<"Num1 is"<<a<<endl;
    cout<<"Num2 is"<<b<<endl;
}
